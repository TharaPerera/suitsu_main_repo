<body class="login-page">
    <div class="login-box">
      <div class="login-logo">
        <a href="#" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <!-- <span class="logo-mini"><img src="images/bim.png"></span> -->
          <!-- logo for regular state and mobile devices -->
           <span class="logo-lg"><img src="themes/htmlBlogAggregator/img/"></span>
        </a>
      </div><!-- /.login-logo -->
      <div class="login-box-body">
<div class="alert alert-error" id="loginError" style="display:none">
                           <span id="loginErrorSpan"></span>
                       </div>
        <p class="login-box-msg">Sign in to start your session</p>
        <form action="dashboard.jag" method="post">
          <div class="form-group has-feedback">
            <input type="username" class="form-control" placeholder="User Name"/>
            <span class="glyphicon glyphicon-user form-control-feedback"></span>
          </div>
          <div class="form-group has-feedback">
            <input type="password" class="form-control" placeholder="Password"/>
            <span class="glyphicon glyphicon-lock form-control-feedback"></span>
          </div>
          <div class="row">
            <div class="col-xs-8">    
         
  <!--   <div class="checkbox icheck">
                <label>
                  <input type="checkbox"> Remember Me
                </label>
              </div>  --> 

                     
            </div><!-- /.col -->
            <div class="col-xs-4">
              <button type="submit" class="btn btn-primary btn-block btn-flat">Sign In</button>
            </div><!-- /.col -->
          </div>
        </form>

        
      <!--   <a href="#" class="text-center">Register a new membership</a> -->

      </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->
